import collections, re
def readUIFile(loc):
    dict=collections.OrderedDict()
    fileText=open(loc,"r").read()
    fileText=re.sub('\n',":",fileText)
    fileArray=fileText.split(":")
    dict[fileArray[0]]=fileArray[1]
    print(dict["Name"])
readUIFile("C:/Users/Taylor/Documents/UI Files/ui1.txt")