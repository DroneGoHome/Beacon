import collections
from Tkinter import *
sys.path.append(r"C:\Users\Taylor\AppData\Local\Programs\Python\Python35-32\Lib\site-packages")

fields=['Name:    ','Phone Number:    ','Cellular Provider:    ','Email Address:    ']
entries=collections.OrderedDict()

def save_entry_fields():
    with open("C:\\Users\\Taylor\\Documents\\UI Files\\UI1.txt",'w') as myfile:
        myfile.write("Name: %s\nPhone Number: %s\n" %(entries[0].get(), entries[1].get()))
    master.destroy()

master = Tk()
for n in range(0,len(fields)):
    entries[fields[n]]=entries[n].get()
    print(entries[n])
    Label(master, text=fields[n]).grid(row=n)
    entries[fields[n]] = Entry(master)
    entries[n].grid(row=1, column=1)
#   e2.grid(row=n, column=1)

Button(master, text='Quit', command=master.quit).grid(row=3, column=0, sticky=W, pady=4)
Button(master, text='Save', command=save_entry_fields).grid(row=3, column=1, sticky=W, pady=4)

mainloop( )